      "browserify-shim",
      "cssify"
    ]
  },
  "browser": {
    "showdown": "./vendor/showdown_for_browserify.js",
    "codemirrorCSS": "./bower_components/codemirror/lib/codemirror.css",
    "angular-ui-codemirror": "./bower_components/angular-ui-codemirror/ui-codemirror.js",
