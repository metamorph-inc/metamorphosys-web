    "gulp-angular-templatecache": "^1.3.0",
    "gulp-buffer": "0.0.2",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.3.4",
    "gulp-js-prettify": "^0.1.0",
    "gulp-jshint": "^1.8.4",
    "gulp-livereload": "^2.1.1",
