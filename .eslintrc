{
    "globals": {
        "angular": true,
        "React": true
    },
    // I want to use babel-eslint for parsing!
    "parser": "babel-eslint",
    "env": {
        // I write for browser
        "browser": true,
        // in CommonJS
        "node": true
    },
    // To give you an idea how to override rule options:
    "rules": {
        "quotes": [gu2, "single"],
        "eol-last": [0],
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "no-trailing-spaces": false,
        "no-extra-strict": false
    }
}
